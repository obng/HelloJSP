<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.mapper.EventMapper">

	<select id="selectEvent" parameterType="event">
		select  title,
				start_date "start",
				end_date "end"
		from  	tbl_event
	</select>

</mapper>